\def\mvsymbol#1{%
  \leavevmode
  \begingroup
  \flagfalse
  \def\process##1##2{%
    \ifstreq{#1}{##1}%
      \flagtrue
      \hbox{\marvosymfont\char'##2}%
    \fi
  }%
  \def\\##1{\process##1}%
  \marvosym@table
  \ifflag
  \else
    \message{marvosym: error: unknown character #1^^J}%
    {\tt <mvsymbol #1>}%
  \fi
  \endgroup
}

