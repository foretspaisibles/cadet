%%% SYNOPSIS

% Martin Vogel's symbols.

% \selectmvsize#1
%  Taille #1 comme 10pt, ou {} pour réinitialiser la taille.

% \mvsymbol#1
%  Où #1 est le nom du symbole.


\font\marvosymfont=fmvr8x

\def\selectmvsize#1{%
  \ifstrempty{#1}%
    \font\marvosymfont=fmvr8x
  \else
    \font\marvosymfont=fmvr8x at #1
  \fi
  \relax
}

\addhook\selectmvsize{\the\textsize pt}\to\textsizeadjustments

